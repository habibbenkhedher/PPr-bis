<Window x:Class="Programm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Programm"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Wählen Sie ein Progamm zu testen:"></Label>
        <ComboBox ItemsSource="{Binding ListProgramm}"  VerticalAlignment="Top" HorizontalAlignment="Left" Width="200" Margin="10,28,0,0">

            <ComboBox.ItemTemplate>

                <DataTemplate>

                    <TextBlock Text="{Binding Path=Name}"></TextBlock> 
                </DataTemplate>
                    
            </ComboBox.ItemTemplate>

        </ComboBox>

    </Grid>
</Window>
